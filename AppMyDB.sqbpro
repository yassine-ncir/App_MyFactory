(
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(50) NOT NULL,
    tel VARCHAR(50) NOT NULL,
    address VARCHAR(50),
    salary DOUBLE NOT NULL,
    factory_name VARCHAR(50),
    FOREIGN KEY (factory_name) REFERENCES Factory(factory_name)
);
CREATE TABLE Products(
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(50) NOT NULL,
    price DOUBLE NOT NULL,
    quantity INT NOT NULL,
    description VARCHAR(100)
);
CREATE TABLE Materials(
    id INT AUTO_INCREMENT PRIMARY KEY,
    itemName VARCHAR(50) NOT NULL,
    unit VARCHAR(50) NOT NULL,
    quantity DOUBLE  NOT NULL
);
CREATE TABLE Factory(
    factory_name VARCHAR(50) PRIMARY KEY ,
    factory_password VARCHAR(50) NOT NULL,
    factory_tel VARCHAR(100) NOT NULL,
    factory_address VARCHAR(50),
    factory_email VARCHAR(50)
);
CREATE TABLE Inventory(
    id INT AUTO_INCREMENT PRIMARY KEY
);
CREATE TABLE Inventory_Products(
    id INT AUTO_INCREMENT PRIMARY KEY,
    product_id INT NOT NULL,
    inventory_id INT NOT NULL,
    FOREIGN KEY (inventory_id) REFERENCES Inventory(id),
    FOREIGN KEY (product_id) REFERENCES Products(id)
);
CREATE TABLE Inventory_Materials(
    id INT AUTO_INCREMENT PRIMARY KEY,
    inventory_id INT not null,
    material_id INT NOT NULL,
    FOREIGN KEY (inventory_id) REFERENCES Inventory(id),
    FOREIGN KEY (material_id) REFERENCES Materials(id)
);
